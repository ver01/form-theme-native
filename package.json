{
    "name": "@ver01/form-theme-native",
    "version": "0.0.2-alpha.1",
    "description": "My Ver01 Form Theme",
    "keywords": [
        "ver01.com",
        "@ver01/form-theme-native"
    ],
    "scripts": {
        "lint": "eslint src",
        "cs-format": "prettier 'src/**/*.js' --write",
        "precommit": "npm run lint && npm run cs-format && git add .",
        "build:lib": "cross-env NODE_ENV=production babel -d lib/ src/",
        "build:css": "touch lib/index.css",
        "build": "rimraf lib && npm run build:lib && npm run build:css",
        "cs-check": "prettier 'src/**/*.js' --check",
        "publish-to-npm": "npm run build && npm publish",
        "preversion": "npm run build && npm run cs-check && npm run lint"
    },
    "main": "lib/index.js",
    "dependencies": {},
    "peerDependencies": {
        "@babel/runtime": "*",
        "react": "*"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.4",
        "@babel/core": "^7.7.0",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
        "@babel/plugin-transform-runtime": "^7.7.4",
        "@babel/preset-env": "^7.7.4",
        "@babel/preset-react": "^7.7.4",
        "babel-eslint": "^10.0.3",
        "cross-env": "^6.0.3",
        "eslint": "^6.7.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.17.0",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.0"
    },
    "license": "MIT"
}
